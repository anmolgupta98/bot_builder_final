<% user_child_node_name = "user_child_node_name" + (@child.id).to_s %>;
<% childnameform =  "childnameform" + (@child.id).to_s %>;
document.getElementById('<%= user_child_node_name %>').classList.remove('hide');
document.getElementById('<%= childnameform %>').classList.add('hide');
document.getElementById('<%= user_child_node_name %>').innerHTML = "<%= j render 'bots/user_child_node_name', child: @child %>";


