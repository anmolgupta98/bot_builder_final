<% bot_node_name = "bot_node_name" + (@node.id).to_s %>;
<% botnameform =  "botnameform" + (@node.id).to_s %>;
document.getElementById('<%= bot_node_name %>').classList.remove('hide');
document.getElementById('<%= botnameform %>').classList.add('hide');
document.getElementById('<%= botnameform %>').value = " ";
document.getElementById('<%= bot_node_name %>').innerHTML = "<%= j render 'bots/bot_node_name', node: @node %>";