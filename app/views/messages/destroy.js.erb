<% showing_previous_messages = "showing_previous_messages" + (@node.id).to_s %>;
<% messages_area = "messages_area" + (@node.id).to_s() %>;
document.getElementById('<%= showing_previous_messages %>').innerHTML = "<%= j render 'bots/showing_previous_messages', delete_button: true, parent_id: @node.parent_id %>";
<% if Message.where(node_id: @node.id, bot_id: @bot.id ) == [] %> 
    document.getElementById('<%= messages_area %>').classList.remove('hide');
<% end %>    